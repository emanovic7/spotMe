<div align="center">
  <h3>Edit Workout</h3>
</div>


<div align="center">
  <%= form_for @workout do |f| %>

    <div>
      <%= f.label :name, "Workout Name" %>
      <%= f.text_field :name %>
    </div>
      </br>

    <div>
      <%= f.label :body_part, "Add Body Part" %>
      <%= f.text_field :body_part %>
    </div>
      </br>

    <div>
      <%= f.label :description, "Describe Workout" %>
      <%= f.text_area :description %>
    </div>

    <div>
      <%= f.submit %>
    </div>

</div>


<%end%>
