<div align="center">
  <h1>Workouts</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><strong>Name</strong></td>
      <td><strong>Level</strong></td>
      <td><strong>Number of Exercises</strong></td>
    </tr>

  <% @workouts.each do |workout| %>
  <tr>
    <td><%= link_to " #{workout.name}", workout_path(workout) %></td>
    <td>Beginner</td>
    <td>12</td>
  </tr>

  <% end %>

</tbody>

</table>
